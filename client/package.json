{
  "name": "ship-management",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.local react-app-rewired start",
    "build": "env-cmd -f .env.local react-app-rewired build",
    "test": "react-app-rewired test",
    "prepush": "npm run format && npm run lint && npm run stylelint",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint:fix": "stylelint \"**/*.scss\" --fix",
    "format": "prettier --check \"src/**/*.ts\" \"src/**/*.tsx\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.ts\" \"src/**/*.tsx\" --fix",
    "api:generate": "rimraf src/app/ship-management-api/api-client && openapi-generator-cli generate -i http://localhost:5000/swagger/v1/swagger.json -o src/app/ship-management-api/api-client -g typescript-axios"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.2",
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.24.0",
    "connected-react-router": "^6.9.2",
    "date-fns": "^2.28.0",
    "env-cmd": "^10.1.0",
    "react": "^18.0.0",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.25.0",
    "react-imask": "^6.4.2",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.0",
    "react-scripts": "5.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.3.1",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@openapitools/openapi-generator-cli": "^2.4.19",
    "@types/react-redux": "^7.1.20",
    "@types/react-router": "^5.1.17",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.3",
    "@types/history": "^4.7.9",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "husky": "^4.3.8",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "sass": "^1.45.0",
    "stylelint": "^14.1.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "customize-cra": "^1.0.0",
    "typescript": "^4.4.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run prepush"
    }
  }
}
